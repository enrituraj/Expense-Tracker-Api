{
    "env": {
        "browser": false,
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended",
        "plugin:import/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn",
        "node/no-unsupported-features/es-syntax": "off",
        "import/no-unresolved": "off"
    }
}
